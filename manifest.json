{
  "manifest_version": 2,
  "name": "WPM",
  "version": "1.0",
  "description": "Extension to track your typing speed in the browser",
  "persistent": false,
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": ["main.js"],
      "css": ["styles.css"]
    }
  ]
}